#include <stdio.h>

    struct nascimento{
    int dia;
    char mes[15];
    int ano;

    };
    struct pessoa
    {
        char nome[50];
        int idade;
        char cidade[20];
        struct nascimento nasc;
    };



    int main(){



    struct pessoa pedro={"pedro kruger",18,"Ponta Grossa",31,"maio",2006};


        printf("%s \n %d \n %s \n\n %d \n %s \n %d", pedro.nome,pedro.idade,pedro.cidade,pedro.nasc);



