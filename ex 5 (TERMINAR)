






#include <stdio.h>

main()
{

    int A[5];
    int B[5];
    int C[10];
    int i;
    int i2;
    int i3;
    for(i=0; i<5; i++)
    {
        printf("Digite um valor para o termo A[%d]: \n",i+1);
        scanf("%d",&A[i]);
    }
    i=0;
    for(i=0; i<5; i++)
    {
        printf("Digite um valor para o termo B[%d]: \n",i+1);
        scanf("%d",&B[i]);
    }
    i=0;
    for(i=0; i<5; i++)
    {
        C[i]=A[i];
    }
    for(i=0; i<5; i++)
    {
        C[i+5]=B[i];
    }
    for(i=0; i<10; i++)
    {
        printf("O elemento C[%d] e: %d \n",i+1,C[i]);
    }
}
