#include <stdio.h>

main(){

    int A[5];
    int B[5];
    int C[10];
    int i;
    int i2;
    int i3;

    for(i=0;i<5;i++){

        printf("Digite um valor para o termo A[%d]: \n",i+1);
        scanf("%d",&A[i]);
    }
    for(i=5;i<10;i++){
        printf("O termo A[%d] e: %d \n",i-5,A[i-5]);
    }

    for(i=10;i<15;i++){

        printf("Digite um valor para o termo B[%d]: \n",i+1-10);
        scanf("%d",&B[i-10]);
    }

    for(i=15;i<20;i++){
        printf("O termo B[%d] e: %d \n",i+1-15,B[i-15]);
    }
    

    for(i2=5;i2<10;i2++){
            
            C[i]=A[i];
        
    }
    
    
        for(i2=10;i2<15;i2++){
            
            C[i+5]=B[i];
        
    }
    
    for(i3=0;i3<10;i3++){
        
        
        printf("O elemento C[%d] e: %d \n",i3+1,C[i]);
    }



}
